{% extends 'base.html.twig' %}

{% block cv %}
    <h2 class="text-center section_title_litle mb-5">
        Mes Soft Skills
    </h2>
    <div class="row align-items-center m-auto p-auto">
        {% for softskill in softskills %}
            <div class="m-1">
                <div class="col-12 soft_skills m-auto badge {{ random(['badge-primary', 'badge-success', 'badge-danger', 'badge-warning', 'badge-info']) }}">{{ softskill.name }}</div>
            </div>
        {% endfor %}
    </div>

{% endblock %}
